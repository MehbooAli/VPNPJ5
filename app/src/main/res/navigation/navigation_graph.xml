    android:id="@+id/navigation_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.vpnpj5.ui.fragments.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.vpnpj5.ui.fragments.setting.SettingFragment"
        android:label="SettingFragment" >
        <action
            android:id="@+id/action_settingFragment_to_myAccountFragment2"
            app:destination="@id/myAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/myAccountFragment"
        android:name="com.example.vpnpj5.ui.fragments.myAccount.MyAccountFragment"
        android:label="MyAccountFragment" />
